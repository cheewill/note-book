{
  "title": "亿级流量电商详情页系统实战",
  "language": "zh-hans",
  "author": "刘凯（kaisesai）",
  "description": "中华石杉老师的亿级流量电商详情页系统实战",
  "plugins": [
    "flexible-alerts",
    "pageview-count",
    "splitter",
    "page-copyright",
    "sharing-plus",
    "custom-favicon",
    "todo",
    "toggle-chapters",
    "multipart",
    "back-to-top-button",
    "code",
    "search-pro",
    "chapter-fold",
    "popup",
    "page-treeview",
    "anchor-navigation-ex",
    "summary"
  ],
  "pluginsConfig": {
    "page-treeview": {
      "copyright": "Copyright &#169; aleen42",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": true,
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}
